{
        "name": "at-messages-parser",
        "version": "0.1.0",
        "description": "A library for parsing AT output",
        "repository": {
                "type": "git",
                "url": "git://github.com/garronej/at-messages-parser.git"
        },
        "main": "./generatedJs/index.js",
        "types": "./generatedJs/index.d.ts",
        "scripts": {
                "postinstall": "npm run build",
                "build": "npm run tsc && npm run jacob",
                "tsc": "tsc -p ./ts/tsconfig.json",
                "jacob": "npm run jacoblex && npm run jacobgram",
                "jacoblex": "jacob -t lexer.jacoblex -l ./generatedJs/Lexer.js",
                "jacobgram": "jacob -g parser.jacobgram -p ./generatedJs/Parser.js",
                "clean": "rm -rf ./generatedJs",
                "test": "node ./generatedJs/example/test"
        },
        "author": "Joseph Garrone",
        "license": "MIT",
        "dependencies": {
                "jacob": "garronej/Jacob",
                "typescript": "^2.1.4"
        },
        "devDependencies": {
                "@types/node": "^4.0.0"
        }
}
