{
        "name": "at-messages-parser",
        "version": "0.7.3",
        "description": "A library for parsing AT output",
        "repository": {
                "type": "git",
                "url": "git://github.com/garronej/at-messages-parser.git"
        },
        "main": "./out/lib/index.js",
        "types": "./out/lib/index.d.ts",
        "scripts": {
                "postinstall": "npm run build",
                "build": "npm run tsc && npm run jacob",
                "tsc": "./node_modules/.bin/tsc -p ./tsconfig.json",
                "jacob": "npm run jacoblex && npm run jacobgram",
                "jacoblex": "jacob -t ./src/lib/lexer.jacoblex -l ./out/lib/Lexer.js",
                "jacobgram": "jacob -g ./src/lib/parser.jacobgram -p ./out/lib/Parser.js",
                "test": "node ./out/test/sample"
        },
        "author": "Joseph Garrone",
        "license": "MIT",
        "dependencies": {
                "@types/node": "^4.0.0",
                "jacob": "garronej/Jacob",
                "typescript": "^2.1.4"
        },
        "devDependencies": {
        }
}
